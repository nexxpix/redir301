<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
    <!-- Admin Configuration -->
    <route id="redir301.config.getRedirections" path="/admin/module/Redir301/getRedirections">
        <default key="_controller">Redir301\Controller\Redir301AdminController::getRedirections</default>
    </route>
    <route id="redir301.config.addRedirection" path="/admin/module/Redir301/addRedirection">
        <default key="_controller">Redir301\Controller\Redir301AdminController::addRedirection</default>
    </route>
    <route id="redir301.config.updateRedirection" path="/admin/module/Redir301/updateRedirection">
        <default key="_controller">Redir301\Controller\Redir301AdminController::updateRedirection</default>
    </route>
    <route id="redir301.config.deleteRedirection" path="/admin/module/Redir301/deleteRedirection">
        <default key="_controller">Redir301\Controller\Redir301AdminController::deleteRedirection</default>
    </route>
</routes>
